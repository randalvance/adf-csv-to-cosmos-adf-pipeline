{
	"name": "ImportCsvToCosmos",
	"properties": {
		"activities": [
			{
				"name": "Data Flow",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "DataFlow",
						"type": "DataFlowReference",
						"parameters": {
							"identifier": {
								"value": "'@{pipeline().parameters.identifier}'",
								"type": "Expression"
							},
							"pipelineRunId": {
								"value": "'@{pipeline().RunId}'",
								"type": "Expression"
							}
						},
						"datasetParameters": {
							"Blob": {
								"fileName": {
									"value": "@concat(pipeline().parameters.identifier,'.csv')",
									"type": "Expression"
								}
							},
							"CosmosDB": {
								"collection": "people"
							},
							"CosmosDBErrors": {
								"collection": "peopleErrors"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"identifier": {
				"type": "string"
			}
		},
		"annotations": []
	}
}